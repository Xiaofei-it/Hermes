<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xiaofei.library.hermestest" >
    <permission android:name="xiaofei.library.hermes.IPC"
        android:label="Label"
        android:description="@string/action_settings"
        android:protectionLevel="dangerous" />
    <uses-permission android:name="xiaofei.library.hermes.IPC"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="xiaofei.library.hermes.internal.HermesService"
            android:enabled="true"
            android:exported="true" >
        </service>

        <activity
            android:name=".test.TestingActivity"
            android:process=":f"
            android:label="@string/title_activity_second" >
        </activity>
        <activity
            android:name=".DemoActivity"
            android:process=":g"
            android:label="@string/title_activity_demo" >
        </activity>
    </application>

</manifest>
